@model dynamic
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title - FDISoft Application</title>
    <link href="/Images/Icons/icon-cts.png" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width">
    <script src="@Url.Content("~/Content/Admin/js/jquery-1.7.2.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/Admin/js/jquery.validate.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/Admin/js/jquery.address-1.4.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/Admin/js/jquery-ui-1.8.19.custom.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/Admin/js/jquery.mask.js")"></script>
    <script src="@Url.Content("~/Content/Admin/js/jquery.multiselect.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/Admin/js/jquery.treeview.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/Admin/js/fileuploader.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/Admin/js/jquery.autocomplete.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/Admin/js/GridView.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/ckeditor/ckeditor.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/ckfinder/ckfinder.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/Admin/js/CarMarket.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/Admin/js/colorpicker.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/Admin/js/admin.js")"></script>
    <script src="@Url.Content("~/Content/Admin/js/Load.js")"></script>
    
    <script src="~/Content/Admin/jquery-timepicker/jquery.timepicker.min.js"></script>
    <link href="~/Content/Admin/jquery-timepicker/jquery.timepicker.css" rel="stylesheet" />
    <link href="/Content/Admin/site.css" rel="stylesheet" type="text/css" />
    <link href="/Content/Admin/css/jquery.multiSelect.css" rel="stylesheet" type="text/css" />
    <link href="/Content/Admin/css/jquery.treeview.css" rel="stylesheet" type="text/css" />
    <link href="/Content/Admin/css/fileuploader.css" rel="stylesheet" type="text/css" />
    <link href="/Content/Admin/css/jquery-ui-1.8.6.gray-green.css" rel="stylesheet" type="text/css" />
    <link href="/Content/Admin/css/gridview.css" rel="stylesheet" type="text/css" />
    <link href="/Content/Admin/css/colorpicker.css" rel="stylesheet" type="text/css" />
    <link href="/Content/Admin/css/jquery.autocomplete.css" rel="stylesheet" type="text/css" />
    <link href="/Content/Admin/css/styles.css" rel="stylesheet" type="text/css" />
    <script type="text/javascript" src="/Uploadify/swfobject.js"></script>
    <script type="text/javascript" src="/Uploadify/jquery.uploadify.v2.1.4.min.js"></script>
    <link href="/Uploadify/uploadify.css" type="text/css" rel="stylesheet" />
    <link href="~/Content/Publishing/css/colorpicker.css" rel="stylesheet" />
    <script src="~/Content/Publishing/js/ColorPicker/colorpicker.js"></script>
    <script src="~/Content/Publishing/js/ColorPicker/eye.js"></script>
    <script src="~/Content/Publishing/js/ColorPicker/utils.js"></script>
    <script src="~/Content/Publishing/js/ColorPicker/layout.js"></script>
    
</head>
<body>
    <div style="display: none;">
        <div id="dialog-confirm">
        </div>
        <div id="dialog-message">
        </div>
        <div id="dialog-form">
        </div>
        <div id="dialog-form-2">
        </div>
        <div id="dialog-form-3">
        </div>
    </div>
    <div id="layout-content">
        <div id="header" role="banner">
            <div class="zone zone-header">
                <div id="branding">
                    <div id="app">
                        <a href="/">Orchard Template</a>
                    </div>
                </div>                
                <h1 id="page-title">Hệ thống quản trị</h1>
            </div>
        </div>
        <div id="navshortcut">
            <a href="#Menu-admin">Skip to navigation </a>
        </div>
        <div class="container">
            <div id="menu">
                <div class="zone zone-navigation">
                    <ul class="menu menu-admin" role="navigation">
                     @* @Html.Action("Index","MenuAdmin")*@
                      
                    </ul>
                </div>
                <div class="sidebar-collapse">&nbsp;</div>
            </div>
            <div id="layout-main">
                <div id="main" role="main">
                    <div id="content" class="group">
                        <div id="login">
                            <div class="clock"></div>
                         
                        </div>
                        <div class="zone zone-content">
                            @RenderBody()
                            <div class="f_modal"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
  
    <script src="@Url.Content("~/Content/Admin/js/jquery.clock.js")" type="text/javascript"></script>
    <script type="text/javascript">
        //<![CDATA[
        $(function () {
            $('.clock').clock();
            $(".menu-admin.menu h3").expandoControl();
            $('.sidebar-collapse').click(function () {
                $('#menu').toggleClass('menu-min');
                $('#layout-content').toggleClass('layout-min');
                if ($(this).hasClass('show-sidebar')) {
                    $(this).removeClass('show-sidebar');
                    $('.showing').css({ 'display': 'block' });
                } else {
                    $(this).addClass('show-sidebar');
                    $('.showing').removeAttr('style');                    
                }
            });

            $('.more-search').click(function() {
                $('.advance-search').slideToggle();
            });
            
            $(".dialog-form").on("dialogcreate", function(event, ui) {
                $("body").css({ overflow: 'hidden' });
            });
            
            $(".dialog-form").on("dialogbeforeclose", function (event, ui) {
                $("body").css({ overflow: 'inherit' });
            });
        })
        //]]>
    </script>
</body>
</html>
