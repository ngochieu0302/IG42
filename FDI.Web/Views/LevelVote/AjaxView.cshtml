@model FDI.Simple.LevelVoteItem
@{
	Layout = "~/Views/Shared/_Ajax.cshtml";
}
<div class="modal-body">
	<div class="portlet box green">
		<div class="portlet-title">
			<div class="caption">
				<i class="fa fa-gear"></i>Thông tin Log
			</div>
		</div>
		<div class="portlet-body form">
			<div class="form-body">
				<table class="table table-bordered">
					<tr>
						<td>
							<label>ID</label>
						</td>
						<td>
							@Html.DisplayFor(model => model.ID)
						</td>
					</tr>

					<tr>
						<td>
							<label>Tên tiêu chí</label>
						</td>
						<td>
							@Html.DisplayFor(model => model.Name)
						</td>
					</tr>

					<tr>
						<td>
							<label>Value</label>
						</td>
						<td>
							@Html.DisplayFor(model => model.Value)
						</td>
					</tr>

					<tr>
						<td>
							<label>Soft</label>
						</td>
						<td>
							@Html.DisplayFor(model => model.Soft)
						</td>
					</tr>
					
					<tr>
						<td>
							<label>Agency ID</label>
						</td>
						<td>
							@Html.DisplayFor(model => model.AgencyID)
						</td>
					</tr>
				</table>
			</div>
		</div>
	</div>
</div>
<div class="modal-footer">
	<button type="button" data-dismiss="modal" class="btn btn-primary btn-sm">Đóng</button>
</div>



