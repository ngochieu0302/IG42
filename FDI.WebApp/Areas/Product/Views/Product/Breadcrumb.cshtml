@model FDI.Simple.BreadcrumbItem
<div class="breadcrumb-area">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <div class="breadcrumb-wrap">
                    <nav aria-label="breadcrumb">
                        <ul class="breadcrumb">
                            <li class="breadcrumb-item"><a href="/"><i class="fa fa-home"></i></a></li>
                            <li class="breadcrumb-item active" aria-current="page">@Model.Breadcrumb</li>
                        </ul>
                    </nav>
                </div>
            </div>
        </div>
    </div>
</div>
