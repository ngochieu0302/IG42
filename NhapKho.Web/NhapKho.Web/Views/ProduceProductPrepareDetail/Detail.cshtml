@{
    ViewBag.Title = "Detail";
}
@using FDI.CORE
@model FDI.Simple.StorageWarehouse.ProduceItem


<div class="portlet yellow-crusta box">
    <div class="portlet-title">
        <div class="caption">
            <i class="fa fa-cogs"></i>Order Details
        </div>
    </div>
    <div class="portlet-body">
        <div class="row static-info">
            <div class="col-md-5 name"> Lệnh sản xuất #: </div>
            <div class="col-md-7 value">
                @Model.ID
            </div>
        </div>
        <div class="row static-info">
            <div class="col-md-5 name"> sản phẩm: </div>
            <div class="col-md-7 value"> @Model.ProductName</div>
        </div>
        <div class="row static-info">
            <div class="col-md-5 name"> Số lượng: </div>
            <div class="col-md-7 value"> @Model.Quantity.Quantity()</div>
        </div>

        <div class="row static-info">
            <div class="col-md-5 name"> Số lượng đã nhập: </div>
            <div class="col-md-7 value"> @Model.QuantityActive.Quantity() </div>
        </div>
    </div>
</div>