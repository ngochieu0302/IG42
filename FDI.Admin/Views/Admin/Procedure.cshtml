@model FDI.Simple.ModelModuleadminItem
@if (Model.ListItem != null && Model.ListItem.Count() > 0)
{
    var i = 1;
    var obj = Model.ListItem.FirstOrDefault(m => m.ID == Model.PrarentID);
    <div class="tabs-menu">
        <strong>@obj.NameModule</strong>
        @foreach (var item in Model.ListItem.Where(c => c.ID != Model.PrarentID))
        {
            <a class="link-menu1 @item.Tag @(item.Tag.ToLower() == Model.Container.ToLower() ? "active" : "")" href="/@item.Tag/p@(item.ID)c@(Model.PrarentID)">@(i++). @item.NameModule<span><i class="fa fa-arrow-right"></i></span></a>
        }
    </div>
}