<!DOCTYPE html>
<!--[if IE 8]> <html lang="vi" class="ie8 no-js"> <![endif]-->
<!--[if IE 9]> <html lang="vi" class="ie9 no-js"> <![endif]-->
<!--[if !IE]><!-->
<html lang="vi">
<!--<![endif]-->
<!-- BEGIN HEAD -->
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link rel="shortcut icon" href="/favicon.ico" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1" name="viewport" />
    <meta content="" name="description" />
    <meta content="" name="author" />
    <!-- BEGIN GLOBAL MANDATORY STYLES -->
    <link href="/Content/Publishing/plugins/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
    <link href="/Content/Publishing/plugins/simple-line-icons/simple-line-icons.min.css" rel="stylesheet" type="text/css" />
    <link href="/Content/Publishing/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="/Content/Publishing/plugins/uniform/css/uniform.default.css" rel="stylesheet" type="text/css" />
    <!-- END GLOBAL MANDATORY STYLES -->
    <!-- BEGIN THEME GLOBAL STYLES -->
    <link href="/Content/Publishing/css/components.css" rel="stylesheet" id="style_components" type="text/css" />
    <!-- END THEME GLOBAL STYLES -->
    <!-- BEGIN THEME LAYOUT STYLES -->
    <link href="/Content/Publishing/css/layout.min.css" rel="stylesheet" type="text/css" />
    <link href="/Content/Publishing/plugins/layout3/css/themes/default.min.css" rel="stylesheet" type="text/css" />
    <link href="/Content/Publishing/plugins/bootstrap-toastr/toastr.min.css" rel="stylesheet" type="text/css">
    <link href="/Content/Publishing/plugins/bootstrap-modal/css/bootstrap-modal-bs3patch.css" rel="stylesheet" type="text/css" />
    <link href="/Content/Publishing/plugins/bootstrap-modal/css/bootstrap-modal.css" rel="stylesheet" type="text/css" />
    <link href="/Content/Publishing/css/custom.min.css" rel="stylesheet" />

    <script src="/Content/Publishing/plugins/jquery.min.js" type="text/javascript"></script>
    <script src="Content/Admin/js/jquery.validate.min.js" type="text/javascript"></script>
    <script src="Content/Admin/js/Common.js"></script>
    <script src="Content/Admin/js/purl.js"></script>
    <link href="/Content/Admin/plugins/select2/css/select2.min.css" rel="stylesheet" />
    <!-- END THEME LAYOUT STYLES -->
</head>
<!-- END HEAD -->
<body class="page-container-bg-solid">
    <!-- BEGIN HEADER -->
    <div class="page-header">
        <!-- BEGIN HEADER TOP -->
        <div class="page-header-top">
            <div class="container">
                <!-- BEGIN LOGO -->
                <div class="page-logo">
                    <a href="/">
                        <img src="/Content/Publishing/img/logo-big.png" alt="logo" class="logo-default">
                    </a>
                </div>
                <!-- END LOGO -->
                <!-- BEGIN RESPONSIVE MENU TOGGLER -->
                <a href="javascript:;" class="menu-toggler"><i class="fa fa-bars"></i></a>
                <!-- END RESPONSIVE MENU TOGGLER -->
                <!-- BEGIN TOP NAVIGATION MENU -->
                <div class="top-menu">
                    <ul class="nav navbar-nav pull-right">
                        @Html.Action("Infomation", "Home")
                    </ul>
                </div>
                <!-- END TOP NAVIGATION MENU -->
            </div>
        </div>
        <!-- END HEADER TOP -->
        <!-- BEGIN HEADER MENU -->
        <div class="page-header-menu">
            <div class="container">
                <!-- BEGIN HEADER SEARCH BOX -->
                @Html.Action("Menu", "Home")
                <!-- END MEGA MENU -->
            </div>
        </div>
        <!-- END HEADER MENU -->
    </div>
    <!-- END HEADER -->
    <!-- BEGIN CONTAINER -->
    <div class="page-container">
        <!-- BEGIN CONTENT -->
        <div class="page-content-wrapper">
            @RenderBody()
        </div>
    </div>
    <!-- END CONTAINER -->
    <!-- BEGIN FOOTER -->
    <!-- BEGIN PRE-FOOTER -->
    <div class="page-prefooter">
        <div class="container">
            <div class="row">
                <div class="col-md-4 col-sm-6 col-xs-12 footer-block">
                    <h2>Về chúng tôi</h2>
                    <p>Internet là một thị trường rất tiềm năng mà bất kỳ doanh nghiệp nào cũng muốn tiếp cận và phát triển thương hiệu thông qua nó.</p>
                </div>
                <div class="col-md-4 col-sm-6 col-xs12 footer-block">
                    <h2>Đăng ký nhận tin</h2>
                    <div class="subscribe-form">
                        <form action="javascript:;">
                            <div class="input-group">
                                <input type="text" placeholder="mail@email.com" class="form-control">
                                <span class="input-group-btn">
                                    <button class="btn" type="submit">Gửi yêu cầu</button>
                                </span>
                            </div>
                        </form>
                    </div>
                </div>
                <div class="col-md-4 col-sm-6 col-xs-12 footer-block">
                    <h2>Liên hệ</h2>
                    <address>
                        Phone: (04)33 838 999 - 0949 699 369
                        <br>
                        Email:
                        <a href="mailto:contact@fditech.vn">contact@fditech.vn</a>
                    </address>
                </div>
            </div>
        </div>
    </div>
    <!-- END PRE-FOOTER -->
    <!-- BEGIN INNER FOOTER -->
    <div class="page-footer">
        <div class="container">2017 &copy; Fditech.vn</div>
    </div>
    <div class="scroll-to-top">
        <i class="icon-arrow-up"></i>
    </div>
    @Html.Partial("~/Views/Shared/Popup.cshtml")
    <!-- END INNER FOOTER -->
    <!-- END FOOTER -->
    <!--[if lt IE 9]>
    <script src="/Content/Publishing/plugins/respond.min.js"></script>
    <script src="/Content/Publishing/plugins/excanvas.min.js"></script>
    <![endif]-->
    <!-- BEGIN CORE PLUGINS -->
    <script src="/Content/Publishing/plugins/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="/Content/Publishing/plugins/bootstrap-sweetalert/sweetalert.min.js" type="text/javascript"></script>
    <script src="/Content/Publishing/plugins/bootstrap-sweetalert/ui-sweetalert.min.js" type="text/javascript"></script>
    <script src="/Content/Publishing/plugins/bootstrap-toastr/toastr.min.js" type="text/javascript"></script>
    <script src="/Content/Publishing/plugins/jquery.blockui.min.js" type="text/javascript"></script>
    <script src="/Content/Publishing/plugins/uniform/jquery.uniform.min.js" type="text/javascript"></script>
    <script src="/Content/Publishing/plugins/bootstrap-modal/js/bootstrap-modalmanager.js" type="text/javascript"></script>
    <script src="/Content/Publishing/plugins/bootstrap-modal/js/bootstrap-modal.js" type="text/javascript"></script>
    <!-- END CORE PLUGINS -->
    <!-- BEGIN THEME GLOBAL SCRIPTS -->
    <script src="/Content/Publishing/plugins/app.min.js" type="text/javascript"></script>
    <!-- END THEME GLOBAL SCRIPTS -->
    <!-- BEGIN THEME LAYOUT SCRIPTS -->
    <script src="/Content//Publishing/plugins/layout3/scripts/layout.min.js" type="text/javascript"></script>
    <script src="/Content//Publishing/plugins/layout3/scripts/demo.min.js" type="text/javascript"></script>
    <!-- END THEME LAYOUT SCRIPTS -->
</body>
</html>
